{
  "name": "reactutilities",
  "author": "Given Suman",
  "license": "MIT",
  "scripts": {
    "lint": "npx eslint src",
    "lint:fix": "npm run lint src --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npx prettier src --write",
    "install:all": "pnpm install:stores && pnpm install:hooks && pnpm install:classnames && pnpm install:components",
    "test:all": "pnpm test:stores && pnpm test:hooks && pnpm test:classnames && pnpm test:components",
    "install:stores": "(cd ./src/stores && pnpm install);",
    "test:stores": "(cd ./src/stores && pnpm test);",
    "install:hooks": "(cd ./src/hooks && pnpm install);",
    "test:hooks": "(cd ./src/hooks && pnpm test);",
    "install:classnames": "(cd ./src/classnames && pnpm install);",
    "test:classnames": "(cd ./src/classnames && pnpm test);",
    "install:css": "(cd ./src/css && pnpm install);",
    "test:css": "(cd ./src/css && pnpm test);",
    "install:components": "(cd ./src/components && pnpm install);",
    "test:components": "(cd ./src/components && pnpm test);"
  },
  "devDependencies": {
    "@types/react": "^18.0.31",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.2"
  }
}
